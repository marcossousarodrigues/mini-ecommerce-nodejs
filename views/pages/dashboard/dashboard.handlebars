<section>
    <div class="dashboard-container">
        <h1>Meus Produtos</h1>
        <p>{{message}}</p>
        {{#each products}}
                <div class="dashboard-item">
                    <p>{{this.name}}<p>
                    <div class="btn-itesm-dashboard">
                        <a href="/product/edit/{{this._id}}">Ver</a>
                        <a href="/product/edit/{{this._id}}">Editar</a>
                        <a href="/product/delete/{{this._id}}">Deletar</a>
                    </div>
            </div>
        {{/each}}

        <a href="/product/create">Adicionar Produto</a>
    </div>
</section>